from google.appengine.ext.db import djangoforms
from django import newforms

from py.model import *

class IspezioneForm(djangoforms.ModelForm):
  
  class Meta:
    model = Ispezione
    exclude = ['creato_il','creato_da','modificato_il','modificato_da','commissario']
    
  
class CommissioneForm(djangoforms.ModelForm):
  class Meta:
    model = Commissione

class CommissioneQueryForm(djangoforms.ModelForm):
  zona = newforms.fields.TextField()
  distretto = newforms.fields.TextField()
    